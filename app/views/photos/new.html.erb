<h1>Post New Photos!</h1>
<br>

<%= form_for @photo do |f| %>

  <div class="field">
    <%= f.label :content, "Comment" %>
    <%= f.text_field :content, :class => "form-control my-form"%>
  </div>
  <br>

  <div class="field">
    <%= f.label :image, "File(s)" %>
    <%= f.file_field :images, multiple: true %>
  </div>
  <br>
  
  <div class="actions">
    <%= f.submit "Post!", :class => "btn btn-default" %>
  </div>
<% end %>